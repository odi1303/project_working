<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.RequestReport">
   <children>
      <Label disable="true" layoutX="46.0" layoutY="37.0" text="Send request for a report">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField editable="false" layoutX="46.0" layoutY="95.0" prefHeight="26.0" prefWidth="498.0" promptText="Enter short description of the report " />
      <Button fx:id="send_button" layoutX="269.0" layoutY="343.0" mnemonicParsing="false" onAction="#send_report" text="send" />
      <TextArea layoutX="47.0" layoutY="133.0" prefHeight="200.0" prefWidth="498.0" promptText="write your full request here" />
      <Button fx:id="back_button" layoutX="14.0" layoutY="360.0" mnemonicParsing="false" onAction="#to_go_back" text="back" />
   </children>
</AnchorPane>
