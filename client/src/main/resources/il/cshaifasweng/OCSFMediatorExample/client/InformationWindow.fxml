<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:controller="il.cshaifasweng.OCSFMediatorExample.client.InformationWindowController"
            xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx/8.0.171"
            prefWidth="300" prefHeight="150">

    <center>
        <Label fx:id="informationLabel" BorderPane.alignment="CENTER"/>
    </center>

    <bottom>
        <Button fx:id="closeButton" text="Close" onAction="#closeWindow" BorderPane.alignment="CENTER" />
    </bottom>

</BorderPane>
